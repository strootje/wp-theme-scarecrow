{
	"private": true,
	"name": "timber.web",
	"version": "0.1.0",
	"scripts": {
		"build": "webpack --progress --colors",
		"serve": "ts-node --files ./src/app/server",
		"prebundle": "yarn build",
		"bundle": "node ./scripts/bundle",
		"compose:up": "docker-compose up -d",
		"compose:down": "docker-compose down",
		"composer": "docker run --rm -it -v $PWD:/app composer install --ignore-platform-reqs --no-scripts"
	},
	"dependencies": {
		"apollo-cache-inmemory": "*",
		"apollo-client": "*",
		"apollo-link": "*",
		"apollo-link-http": "*",
