{
	"private": true,
	"name": "timber.web",
	"version": "0.1.0",
	"scripts": {
		"build": "webpack --progress --colors",
		"serve": "ts-node ./src/app/server",
		"prebundle": "yarn build",
		"bundle": "node ./scripts/bundle",
		"compose:up": "docker-compose up -d",
		"compose:down": "docker-compose down",
		"composer": "docker run --rm -it -v $PWD:/app composer install --ignore-platform-reqs --no-scripts"
	},
	"dependencies": {
		"react": "*",
		"react-dom": "*",
		"react-redux": "*",
		"redux": "*"
	},
	"devDependencies": {
		"@types/html-webpack-plugin": "*",
		"@types/mini-css-extract-plugin": "*",
		"@types/node": "*",
		"@types/react": "*",
		"@types/react-dom": "*",
		"@types/react-redux": "*",
		"@types/webpack": "*",
		"@types/webpack-serve": "*",
		"archiver": "*",
		"css-loader": "*",
		"html-webpack-plugin": "*",
		"mini-css-extract-plugin": "*",
		"node-sass": "*",
		"sass-loader": "*",
		"skeleton-sass-official": "*",
		"source-map-loader": "*",
		"ts-loader": "*",
		"ts-node": "*",
		"typescript": "*",
		"webpack": "*",
		"webpack-cli": "*",
		"webpack-serve": "*"
	}
}
