		"prop-types": "*",
		"react": "*",
		"react-apollo": "*",
		"react-dom": "*",
		"react-intl": "*",
		"react-redux": "*",
		"redux": "*",
		"redux-logger": "*",
		"redux-thunk": "*"
	},
	"devDependencies": {
		"@types/archiver": "*",
		"@types/connect-history-api-fallback": "*",
		"@types/graphql": "*",
		"@types/html-webpack-plugin": "*",
		"@types/mini-css-extract-plugin": "*",
		"@types/node": "*",
		"@types/react": "*",
		"@types/react-dom": "*",
		"@types/react-intl": "*",
		"@types/react-redux": "*",
